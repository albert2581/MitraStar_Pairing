<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.kerorodoodesk.mtapp.RemoteActivity">


    <ImageButton
        android:id="@+id/power"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignStart="@+id/three"
        android:background="@null"
        android:contentDescription="@null"
        android:onClick="onClickpower"
        android:scaleType="centerCrop"
        android:src="@drawable/change_power" />

    <ImageButton
        android:id="@+id/one"
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="60dp"
        android:background="@null"
        android:contentDescription="@null"
        android:onClick="onClickOne"
        android:scaleType="centerCrop"
        android:src="@drawable/change_one" />

    <ImageButton
        android:id="@+id/two"
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:layout_alignTop="@+id/one"
        android:layout_centerHorizontal="true"
        android:background="@null"
        android:contentDescription="@null"
        android:onClick="onClicktwo"
        android:scaleType="centerCrop"
        android:src="@drawable/change_two" />

    <ImageButton
        android:id="@+id/four"
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:layout_alignTop="@+id/one"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="60dp"
        android:background="@null"
        android:contentDescription="@null"
        android:onClick="onClickthree"
        android:scaleType="centerCrop"
        android:src="@drawable/change_four" />

    <ImageButton
        android:id="@+id/five"
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:layout_alignBottom="@+id/four"
        android:layout_alignStart="@+id/two"
        android:background="@null"
        android:contentDescription="@null"
        android:onClick="onClickfour"
        android:scaleType="centerCrop"
        android:src="@drawable/change_five" />

    <ImageButton
        android:id="@+id/three"
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/two"
        android:layout_marginRight="30dp"
        android:background="@null"
        android:contentDescription="@null"
        android:onClick="onClickfive"
        android:scaleType="centerCrop"
        android:src="@drawable/change_three" />

    <ImageButton
        android:id="@+id/six"
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:layout_alignBottom="@+id/five"
        android:layout_alignStart="@+id/three"
        android:background="@null"
        android:contentDescription="@null"
        android:onClick="onClicksix"
        android:scaleType="centerCrop"
        android:src="@drawable/change_six" />

    <ImageButton
        android:id="@+id/seven"
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/four"
        android:layout_marginLeft="30dp"
        android:background="@null"
        android:contentDescription="@null"
        android:onClick="onClickseven"
        android:scaleType="centerCrop"
        android:src="@drawable/change_seven" />

    <ImageButton
        android:id="@+id/eight"
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:layout_alignBottom="@+id/seven"
        android:layout_alignStart="@+id/five"
        android:background="@null"
        android:contentDescription="@null"
        android:onClick="onClickeight"
        android:scaleType="centerCrop"
        android:src="@drawable/change_eight" />

    <ImageButton
        android:id="@+id/nine"
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:layout_alignBottom="@+id/seven"
        android:layout_alignStart="@+id/six"
        android:background="@null"
        android:contentDescription="@null"
        android:onClick="onClicknine"
        android:scaleType="centerCrop"
        android:src="@drawable/change_nine" />

    <ImageButton
        android:id="@+id/zero"
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:layout_alignBottom="@+id/enter_button"
        android:layout_alignStart="@+id/five"
        android:background="@null"
        android:contentDescription="@null"
        android:onClick="onClickzero"
        android:scaleType="centerCrop"
        android:src="@drawable/change_zero" />

    <ImageButton
        android:id="@+id/back"
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:layout_alignBottom="@+id/enter_button"
        android:layout_alignStart="@+id/three"
        android:background="@null"
        android:contentDescription="@null"
        android:onClick="onClickback"
        android:scaleType="centerCrop"
        android:src="@drawable/change_back" />

    <ImageButton
        android:id="@+id/voiceUp"
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/enter_button"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="10dp"
        android:background="@null"
        android:contentDescription="@null"
        android:onClick="onClickvoiceUp"
        android:scaleType="centerCrop"
        android:src="@drawable/change_vol_up" />

    <ImageButton
        android:id="@+id/voiceDown"
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/enter_button"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="70dp"
        android:background="@null"
        android:contentDescription="@null"
        android:onClick="onClickvoiceDown"
        android:scaleType="centerCrop"
        android:src="@drawable/change_vol_down" />


    <ImageButton
        android:id="@+id/voiceUpMiddle"
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/enter_button"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="40dp"
        android:background="@null"
        android:contentDescription="@null"
        android:onClick="onClickvoiceUp"
        android:scaleType="centerCrop"
        android:src="@drawable/button_vol" />


    <ImageButton
        android:id="@+id/enter_button"
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/seven"
        android:layout_marginLeft="30dp"
        android:background="@null"
        android:contentDescription="@null"
        android:onClick="onClickenter_button"
        android:scaleType="centerCrop"
        android:src="@drawable/change_ok" />

    <ImageButton
        android:id="@+id/up_button"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_alignBottom="@+id/voiceUp"
        android:layout_alignStart="@+id/zero"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="10dp"
        android:background="@null"
        android:contentDescription="@null"
        android:onClick="onClickup_button"
        android:scaleType="centerCrop"
        android:src="@drawable/change_ch_up" />

    <ImageButton
        android:id="@+id/down_button"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_alignStart="@+id/zero"
        android:layout_below="@+id/zero"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="100dp"
        android:background="@null"
        android:contentDescription="@null"
        android:onClick="onClickdown_button"
        android:scaleType="centerCrop"
        android:src="@drawable/change_down" />

    <ImageButton
        android:id="@+id/right_button"
        android:layout_width="50dp"
        android:layout_height="40dp"
        android:layout_alignStart="@+id/up_button"
        android:layout_below="@+id/up_button"
        android:layout_marginLeft="40dp"
        android:background="@null"
        android:contentDescription="@null"
        android:onClick="onClickright_button"
        android:scaleType="centerCrop"
        android:src="@drawable/change_right" />

    <ImageButton
        android:id="@+id/left_button"
        android:layout_width="50dp"
        android:layout_height="40dp"
        android:layout_below="@+id/up_button"
        android:layout_marginLeft="100dp"
        android:background="@null"
        android:contentDescription="@null"
        android:onClick="onClickleft_button"
        android:scaleType="centerCrop"
        android:src="@drawable/change_left" />

    <ImageButton
        android:id="@+id/channelUp"
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:layout_alignBottom="@+id/voiceUp"
        android:layout_alignStart="@+id/back"
        android:layout_marginRight="30dp"
        android:background="@null"
        android:contentDescription="@null"
        android:onClick="onClickchannelUp"
        android:scaleType="centerCrop"
        android:src="@drawable/change_ch_up" />

    <ImageButton
        android:id="@+id/channelDown"
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:layout_alignBottom="@+id/voiceDown"
        android:layout_alignStart="@+id/back"
        android:layout_marginLeft="0dp"
        android:background="@null"
        android:contentDescription="@null"
        android:onClick="onClickchannelDown"
        android:scaleType="centerCrop"
        android:src="@drawable/change_ch_down" />

    <ImageButton
        android:id="@+id/channelMiddle"
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:layout_alignBottom="@+id/voiceUpMiddle"
        android:layout_alignStart="@+id/back"
        android:background="@null"
        android:contentDescription="@null"
        android:onClick="onClickchannelUp"
        android:scaleType="centerCrop"
        android:src="@drawable/button_ch" />


    <ImageButton
        android:id="@+id/fast"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignBottom="@+id/forward"
        android:layout_alignStart="@+id/left_button"
        android:layout_marginLeft="12dp"
        android:background="@null"
        android:contentDescription="@null"
        android:onClick="onClickfast"
        android:scaleType="centerCrop"
        android:src="@drawable/change_fastforward" />

    <ImageButton
        android:id="@+id/forward"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/voiceDown"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="20dp"
        android:background="@null"
        android:contentDescription="@null"
        android:onClick="onClickforward"
        android:scaleType="centerCrop"
        android:src="@drawable/change_turnback" />

